{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch via ngCLI",
			"runtimeExecutable": "ng",
			"windows": {
				"runtimeExecutable": "ng.cmd"
			},
			"runtimeArgs": [
				"serve",
				"--port",
				"5000"
			],
			"port": 5858
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome against localhost",
			"url": "http://localhost:5000",
			"webRoot": "${workspaceRoot}"
		},
		{
			"type": "chrome",
			"request": "attach",
			"name": "Attach to Chrome",
			"port": 9222,
			"webRoot": "${workspaceRoot}"
		}
	]
}
